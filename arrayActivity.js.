// Create an array named mixedArr with different data types
const mixedArr = ["string", ["otherarray"], 123, true];

// Access and log each item in mixedArr using array index notation
console.log(mixedArr[0]); // Output: string
console.log(mixedArr[1]); // Output: ["otherarray"]
console.log(mixedArr[2]); // Output: 123
console.log(mixedArr[3]); // Output: true
